{
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "script",
        "ecmaFeatures": {}
    },
    "rules": {
        "constructor-super": 2,
        "no-case-declarations": 2,
        "no-class-assign": 2,
        "no-compare-neg-zero": 2,
        "no-cond-assign": 2,
        "no-const-assign": 2,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-dupe-args": 2,
        "no-dupe-class-members": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-empty-pattern": 2,
        "no-empty": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-fallthrough": 2,
        "no-func-assign": 2,
        "no-global-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-new-symbol": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-self-assign": 2,
        "no-sparse-arrays": 2,
        "no-this-before-super": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "no-unsafe-finally": 2,
        "no-unsafe-negation": 2,
        "no-unused-labels": 2,
        "no-useless-escape": 2,
        "require-yield": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "no-extra-semi": 2,
        "semi-style": 2,
        "no-trailing-spaces": 2,
        "arrow-spacing": 2,
        "semi": [2, "never"],
        "accessor-pairs": 2,
        "brace-style": 2,
        "callback-return": 2,
        "class-methods-use-this": 2,
        "comma-dangle": 2,
        "comma-spacing": 2,
        "comma-style": 2,
        "complexity": 2,
        "computed-property-spacing": 2,
        "consistent-return": 2,
        "consistent-this": 2,
        "block-spacing": 2,
        "block-scoped-var": 2,
        "curly": 2,
        "default-case": 2,
        "dot-notation": 2,
        "eol-last": 2,
        "eqeqeq": 2,
        "for-direction": 2,
        "func-call-spacing": 2,
        "func-name-matching": 2,
        "generator-star-spacing": 2,
        "getter-return": 2,
        "global-require": 2,
        "guard-for-in": 2,
        "id-blacklist": 2,
        "id-match": 2,
        "indent": [2, 2],
        "key-spacing": 2,
        "keyword-spacing": [2, {"before": true, "after": true}],
        "no-div-regex": 2,
        "no-duplicate-imports": 2,
        //warning about e.g.: own_container, part of Safe api
        //"camelcase": 2,
        "no-eq-null": 2,
        "no-extra-parens": 2,
        "no-mixed-operators": 2,
        "no-multi-spaces": 2,
        "no-self-compare": 2,
        "no-tabs": 2,
        "no-unmodified-loop-condition": 2,
        "no-useless-concat": 2,
        "no-useless-constructor": 2,
        "no-useless-call": 2,
        "no-useless-computed-key": 2,
        "no-use-before-define": 2,
        "no-useless-return": 2,
        "one-var-declaration-per-line": 2,
        "prefer-destructuring": 2,
        "prefer-const": 2,
        "switch-colon-spacing": 2,
        "quotes": ["error", "single"],
        "valid-jsdoc": 2,
#conflict with .then(_ => ...
#        "no-unused-vars": 2,
        "space-before-function-paren": ["error", "always"],
        #"space-infix-ops": 2,
        "handle-callback-err": 2,        
        "no-undef": 2,
        "no-multiple-empty-lines": 2,
        "operator-linebreak": ["error", "before"],
        "one-var": ["error", "never"],
        "dot-location": 2,
        #conflicts with 'await', it seems
        #"new-cap": 2,
        "new-parens": 2,
        "no-array-constructor": 2,
        "no-caller": 2,
        "no-eval": 2,
        "no-extra-bind": 2,
        "no-floating-decimal": 2,
        "no-iterator": 2,
        "no-label-var": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-multi-str": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-require": 2,
        "no-new-wrappers": 2,
        "no-octal-escape": 2,
        "no-path-concat": 2,
        "no-return-assign": 2,
        "no-sequences": 2,
        "no-shadow-restricted-names": 2,
        "no-template-curly-in-string": 2,
        "no-undef-init": 2,
        "no-unneeded-ternary": 2,
        "no-useless-rename": 2,
        "no-whitespace-before-property": 2,
        "no-with": 2,
        "object-property-newline": 2,
        "padded-blocks": ["error", "never"],
        "rest-spread-spacing": 2,
        "semi-spacing": 2,
        "space-before-blocks": 2,
        "space-unary-ops": 2,
        "spaced-comment": 2,
        "template-curly-spacing": 2,
        "wrap-iife": 2,
        "yield-star-spacing": ["error", {"before": true, "after": true}],
        "yoda": 2
    },
    "env": {
        "browser": true,
        "es6": true
    }
}
